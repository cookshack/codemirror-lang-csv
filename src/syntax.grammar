@top Program { (Row newline)* (Row (newline | eof)) }

@skip {}

Row { expression* }

expression {
  String |
  Separator |
  Value
}

@tokens {
  Separator { $[,;] }

  Value { ![,;\r\n]+ }

  String { '"' (!["\\] | "\\" _)* '"' }

  newline { '\r'? '\n' }
  eof { @eof }

  @precedence { String, Separator, Value }
}

@detectDelim
